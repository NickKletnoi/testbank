{
  "family": "simplebank",
  "containerDefinitions": [
    {
      "name": "web",
      "image": "east-1.amazonaws.com/simplebank:latest",
      "cpu": 128,
      "memoryReservation": 128,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
